<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <net.daum.mf.map.api.MapView
        android:id="@+id/map_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="@dimen/main_search_margin"
        android:gravity="center">

        <LinearLayout
            android:id="@+id/btn_search"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/search_input"
            android:gravity="center"
            android:paddingStart="@dimen/main_search_padding_horizontal"
            android:paddingEnd="@dimen/main_search_padding_horizontal"
            android:paddingTop="5dp"
            android:paddingBottom="5dp">

            <TextView
                android:id="@+id/tv_search"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                tools:text="검색어를 입력하세요."
                android:backgroundTint="@color/invisible"
                android:textColor="@color/dark"
                android:textSize="@dimen/main_search_text"
                android:fontFamily="@font/noto_sans_cjk_kr_medium" />

            <ImageView
                android:layout_width="@dimen/small_icon"
                android:layout_height="@dimen/small_icon"
                android:src="@drawable/main_search"/>
        </LinearLayout>

        <Space
            android:layout_width="@dimen/main_search_margin"
            android:layout_height="0dp" />

        <LinearLayout
            android:id="@+id/btn_filter"
            android:layout_width="@dimen/main_settings_icon"
            android:layout_height="@dimen/main_settings_icon"
            android:orientation="horizontal"
            android:background="@drawable/search_input"
            android:gravity="center">

            <ImageView
                android:layout_width="@dimen/small_icon"
                android:layout_height="@dimen/small_icon"
                android:src="@drawable/main_settings" />
        </LinearLayout>
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/main_underbar"
            android:adjustViewBounds="true"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Space
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center">

                    <ImageView
                        android:id="@+id/btn_register_area"
                        android:layout_width="@dimen/large_icon"
                        android:layout_height="@dimen/large_icon"
                        android:src="@drawable/register_area" />
                </LinearLayout>

                <Space
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center">

                    <ImageView
                        android:id="@+id/btn_mypage"
                        android:layout_width="@dimen/large_icon"
                        android:layout_height="@dimen/large_icon"
                        android:src="@drawable/mypage"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/btn_gps"
            android:layout_width="@dimen/main_gps_icon"
            android:layout_height="@dimen/main_gps_icon"
            android:src="@drawable/gps_locate"
            android:layout_gravity="center"/>
    </FrameLayout>

    <LinearLayout
        android:id="@+id/view_result"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/btn_cancel_result"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:background="@android:color/white">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/result_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/view_place"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone"
        tools:visibility="gone">

        <LinearLayout
            android:id="@+id/btn_cancel_place"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/white"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/btn_expand"
                android:layout_width="@dimen/small_icon"
                android:layout_height="@dimen/small_icon"
                android:layout_gravity="center"
                android:src="@drawable/expand" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="@dimen/topbar_padding_horizontal"
                android:paddingEnd="@dimen/topbar_padding_horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_gravity="bottom|center">

                    <TextView
                        android:id="@+id/tv_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="국가 흡연부스 (폐쇄형)"
                        android:textColor="@color/dark"
                        android:textSize="@dimen/detail_type_text"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold" />

                    <TextView
                        android:id="@+id/tv_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="을지로 입구역 8번출구"
                        android:textColor="@color/dark"
                        android:textSize="@dimen/detail_name_text"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold" />
                </LinearLayout>

                <CheckBox
                    android:id="@+id/cb_bookmark"
                    android:layout_width="@dimen/bookmark_small_width"
                    android:layout_height="@dimen/bookmark_small_height"
                    android:background="@drawable/bookmark"
                    android:button="@color/invisible"
                    android:layout_gravity="center|end"/>
            </FrameLayout>

            <TextView
                android:id="@+id/tv_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="서울 특별시 중구 을지로 1가"
                android:textColor="@color/gray"
                android:textSize="@dimen/detail_address_text"
                android:fontFamily="@font/noto_sans_cjk_kr_regular"
                android:layout_marginTop="@dimen/detail_address_margin_vertical"
                android:layout_marginBottom="@dimen/detail_address_margin_vertical"
                android:layout_gravity="center" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">

                <Space
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/cb_thumb"
                        android:layout_width="@dimen/medium_icon"
                        android:layout_height="@dimen/medium_icon"
                        android:background="@drawable/thumb"
                        android:button="@color/invisible" />

                    <TextView
                        android:id="@+id/tv_thumb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="21"
                        android:textColor="@color/dark"
                        android:textSize="@dimen/detail_thumb_text"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold" />
                </LinearLayout>

                <Space
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <CheckBox
                            android:id="@+id/cb_density_1"
                            android:layout_width="@dimen/detail_density_width"
                            android:layout_height="@dimen/detail_density_height"
                            android:background="@drawable/density"
                            android:enabled="false"
                            android:button="@color/invisible" />

                        <CheckBox
                            android:id="@+id/cb_density_2"
                            android:layout_width="@dimen/detail_density_width"
                            android:layout_height="@dimen/detail_density_height"
                            android:background="@drawable/density"
                            android:enabled="false"
                            android:button="@color/invisible" />

                        <CheckBox
                            android:id="@+id/cb_density_3"
                            android:layout_width="@dimen/detail_density_width"
                            android:layout_height="@dimen/detail_density_height"
                            android:background="@drawable/density"
                            android:enabled="false"
                            android:button="@color/invisible" />

                        <CheckBox
                            android:id="@+id/cb_density_4"
                            android:layout_width="@dimen/detail_density_width"
                            android:layout_height="@dimen/detail_density_height"
                            android:background="@drawable/density"
                            android:enabled="false"
                            android:button="@color/invisible" />

                        <CheckBox
                            android:id="@+id/cb_density_5"
                            android:layout_width="@dimen/detail_density_width"
                            android:layout_height="@dimen/detail_density_height"
                            android:background="@drawable/density"
                            android:enabled="false"
                            android:button="@color/invisible" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/density_low"
                            android:textColor="@color/dark"
                            android:textSize="@dimen/detail_density_text"
                            android:fontFamily="@font/noto_sans_cjk_kr_bold" />

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/density_medium"
                            android:textColor="@color/dark"
                            android:textSize="@dimen/detail_density_text"
                            android:fontFamily="@font/noto_sans_cjk_kr_bold" />

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/density_high"
                            android:textColor="@color/dark"
                            android:textSize="@dimen/detail_density_text"
                            android:fontFamily="@font/noto_sans_cjk_kr_bold" />
                    </LinearLayout>
                </LinearLayout>

                <Space
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1" />
            </LinearLayout>

            <TableLayout
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/main_place_btn_margin_vertical"
                android:layout_marginStart="@dimen/main_review_margin"
                android:layout_marginEnd="@dimen/main_review_margin">

                <TableRow android:gravity="center">
                    <TextView
                        android:id="@+id/tv_review_user_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="구파발발이(dksjqos23)"
                        android:textSize="@dimen/review_id_text"
                        android:textColor="@color/dark"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold"/>

                    <Space
                        android:layout_width="@dimen/main_review_margin"
                        android:layout_height="0dp"/>

                    <TextView
                        android:id="@+id/tv_review_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="다른 데보다 여기가 확실히 깨끗해요"
                        android:textSize="@dimen/review_text"
                        android:textColor="@color/dark"
                        android:fontFamily="@font/noto_sans_cjk_kr_medium"/>
                </TableRow>

                <TableRow android:gravity="center">
                    <TextView
                        android:id="@+id/tv_review_user_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="니냐니뇨 (d__d98)"
                        android:textSize="@dimen/review_id_text"
                        android:textColor="@color/dark"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold"/>

                    <Space
                        android:layout_width="@dimen/main_review_margin"
                        android:layout_height="0dp"/>

                    <TextView
                        android:id="@+id/tv_review_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="앉아 있을 공간이 없음."
                        android:textSize="@dimen/review_text"
                        android:textColor="@color/dark"
                        android:fontFamily="@font/noto_sans_cjk_kr_medium"/>
                </TableRow>
            </TableLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingStart="@dimen/detail_btn_space"
                android:paddingEnd="@dimen/detail_btn_space"
                android:layout_marginTop="@dimen/main_place_btn_margin_vertical"
                android:layout_marginBottom="@dimen/main_place_btn_margin_vertical">

                <LinearLayout
                    android:id="@+id/btn_navigate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:background="@drawable/btn_detail"
                    android:gravity="center"
                    android:paddingTop="@dimen/detail_btn_padding_vertical"
                    android:paddingBottom="@dimen/detail_btn_padding_vertical">

                    <ImageView
                        android:layout_width="@dimen/small_icon"
                        android:layout_height="@dimen/small_icon"
                        android:src="@drawable/navigation"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/detail_navigation"
                        android:textColor="@android:color/white"
                        android:textSize="@dimen/detail_btn_text"
                        android:fontFamily="@font/noto_sans_cjk_kr_medium"
                        android:layout_marginStart="@dimen/detail_btn_text_space" />
                </LinearLayout>

                <Space
                    android:layout_width="@dimen/detail_btn_space"
                    android:layout_height="0dp" />

                <LinearLayout
                    android:id="@+id/btn_share"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:background="@drawable/btn_detail"
                    android:gravity="center"
                    android:paddingTop="@dimen/detail_btn_padding_vertical"
                    android:paddingBottom="@dimen/detail_btn_padding_vertical">

                    <ImageView
                        android:layout_width="@dimen/small_icon"
                        android:layout_height="@dimen/small_icon"
                        android:src="@drawable/share"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/detail_share"
                        android:textColor="@android:color/white"
                        android:textSize="@dimen/detail_btn_text"
                        android:fontFamily="@font/noto_sans_cjk_kr_medium"
                        android:layout_marginStart="@dimen/detail_btn_text_space"  />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/main_banner"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/shadowed"
        tools:visibility="gone"
        android:gravity="center" >

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/main_banner_margin" >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/main_banner"
                android:adjustViewBounds="true"/>

            <ImageView
                android:id="@+id/btn_close_banner"
                android:layout_width="@dimen/main_banner_close"
                android:layout_height="@dimen/main_banner_close"
                android:src="@drawable/close_banner"
                android:layout_margin="@dimen/main_banner_close_margin"
                android:layout_gravity="top|end"/>
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/view_filter"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/shadowed"
        android:visibility="gone"
        tools:visibility="visible"
        android:gravity="center">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/popup_box"
            android:layout_marginStart="@dimen/area_popup_margin_horizontal"
            android:layout_marginEnd="@dimen/area_popup_margin_horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="@dimen/area_checkbox_margin">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="@dimen/small_icon"
                        android:layout_height="@dimen/small_icon"
                        android:src="@drawable/icon_opened"
                        android:layout_marginEnd="@dimen/small_space"/>

                    <CheckBox
                        android:id="@+id/cb_type_opened"
                        style="@style/PlaceCheckBox"
                        android:text="@string/area_type_opened" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="@dimen/small_icon"
                        android:layout_height="@dimen/small_icon"
                        android:src="@drawable/icon_closed"
                        android:layout_marginEnd="@dimen/small_space"/>

                    <CheckBox
                        android:id="@+id/cb_type_closed"
                        style="@style/PlaceCheckBox"
                        android:text="@string/area_type_closed" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="@dimen/small_icon"
                        android:layout_height="@dimen/small_icon"
                        android:src="@drawable/icon_fullyclosed"
                        android:layout_marginEnd="@dimen/small_space"/>

                    <CheckBox
                        android:id="@+id/cb_type_fullyclosed"
                        style="@style/PlaceCheckBox"
                        android:text="@string/area_type_fullyclosed" />
                </LinearLayout>
            </LinearLayout>

            <View
                style="@style/SplitLineHorizontal"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/btn_close"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:text="@string/decision_close"
                    android:textColor="@color/half_dark"
                    android:gravity="center"
                    android:fontFamily="@font/noto_sans_cjk_kr_black"
                    android:textSize="@dimen/area_decision_text"
                    android:paddingTop="@dimen/area_decision_padding_vertical"
                    android:paddingBottom="@dimen/area_decision_padding_vertical"/>

                <View
                    style="@style/SplitLineVertical" />

                <TextView
                    android:id="@+id/btn_confirm"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:text="@string/decision_confirm"
                    android:textColor="@color/dark"
                    android:gravity="center"
                    android:fontFamily="@font/noto_sans_cjk_kr_black"
                    android:textSize="@dimen/area_decision_text"
                    android:paddingTop="@dimen/area_decision_padding_vertical"
                    android:paddingBottom="@dimen/area_decision_padding_vertical"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/area_popup_margin_horizontal"
            android:layout_marginEnd="@dimen/area_popup_margin_horizontal"
            android:layout_marginTop="@dimen/area_information_margin_vertical"
            android:layout_marginBottom="@dimen/area_information_margin_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/btn_information"
                android:layout_width="@dimen/medium_icon"
                android:layout_height="@dimen/medium_icon"
                android:src="@drawable/information"/>

            <LinearLayout
                android:id="@+id/view_information"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="invisible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/information_title_opened"
                        android:textColor="@android:color/white"
                        android:shadowRadius="5.0"
                        android:shadowColor="@color/shadowed_highlight"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold"
                        android:textSize="@dimen/area_information_title_text"
                        android:layout_marginEnd="@dimen/area_information_title_margin_end"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/information_opened"
                        android:textColor="@android:color/white"
                        android:fontFamily="@font/noto_sans_cjk_kr_regular"
                        android:textSize="@dimen/area_information_title_text"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/information_title_closed"
                        android:textColor="@android:color/white"
                        android:shadowRadius="5.0"
                        android:shadowColor="@color/shadowed_highlight"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold"
                        android:textSize="@dimen/area_information_title_text"
                        android:layout_marginEnd="@dimen/area_information_title_margin_end"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/information_closed"
                        android:textColor="@android:color/white"
                        android:fontFamily="@font/noto_sans_cjk_kr_regular"
                        android:textSize="@dimen/area_information_title_text"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/information_title_fullyclosed"
                        android:textColor="@android:color/white"
                        android:shadowRadius="5.0"
                        android:shadowColor="@color/shadowed_highlight"
                        android:fontFamily="@font/noto_sans_cjk_kr_bold"
                        android:textSize="@dimen/area_information_title_text"
                        android:layout_marginEnd="@dimen/area_information_title_margin_end"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/information_fullyclosed"
                        android:textColor="@android:color/white"
                        android:fontFamily="@font/noto_sans_cjk_kr_regular"
                        android:textSize="@dimen/area_information_title_text"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>